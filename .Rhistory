ratio[[i]] <- no.hits.coop[[i]]/no.hits.total[[i]]
if(is.na(no.hits.total[[i]]))
no.hits.total[[i]] <- 0
if(is.na(no.hits.coop[[i]]))
{
no.hits.coop[[i]] <- 0
ratio[[i]] <- 0
}
table_results[i, 1:3] <- c(no.hits.total[[i]], no.hits.coop[[i]], ratio[[i]])
}
table_results
}
